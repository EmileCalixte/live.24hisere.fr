#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use App\MainApp;
use Symfony\Component\Console\Application;

// Initialize MainApp to initialize database from configuration
(MainApp::getInstance());

$application = new Application();

// Register commands
$application->add(new \App\Command\CreateUser\CreateUserCommand());
$application->add(new \App\Command\ImportRunners\ImportRunnersCommand());

$application->run();
