{
  "name": "@live24hisere/backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "create-user": "node dist/src/cli.js create-user",
    "update-password": "node dist/src/cli.js update-user-password",
    "anonymize-runners": "node dist/src/cli.js anonymize-runners",
    "import-passages-csv": "node dist/src/cli.js import-passages-csv",
    "drizzle-kit": "drizzle-kit",
    "createmigration": "drizzle-kit generate --name",
    "migrate": "node dist/src/cli.js migrate",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "pnpm start:dev",
    "start": "nest start",
    "start:dev": "pnpm nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typecheck": "tsc --noEmit",
    "test": "exit 0",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts"
  },
  "dependencies": {
    "@live24hisere/core": "workspace:^",
    "@live24hisere/utils": "workspace:^",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.13",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.13",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.13",
    "@nestjs/schedule": "^6.1.1",
    "@nestjs/swagger": "^11.2.6",
    "@types/papaparse": "^5.5.2",
    "argon2": "^0.44.0",
    "axios": "^1.13.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cron": "^4.4.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "drizzle-orm": "^0.45.1",
    "mysql2": "^3.16.3",
    "nest-commander": "^3.20.1",
    "papaparse": "^5.5.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.13",
    "@swc/core": "^1.15.11",
    "@types/express": "^5.0.6",
    "@types/node": "22.19.8",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.18",
    "drizzle-kit": "^0.31.8",
    "jest-extended": "^7.0.0",
    "prettier": "^3.8.1",
    "source-map-support": "^0.5.20",
    "supertest": "^7.2.2",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.18"
  }
}
